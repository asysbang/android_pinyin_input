<?xml version="1.0" encoding="UTF-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:drawable="@color/colorTransparent" />
    <item android:state_pressed="true" android:drawable="@color/colorTransparent" />

    <item>
        <shape>
            <stroke android:width="3px" android:color="#ffa8b0bc"/>
            <solid android:color="@color/colorTransparent"/>
        </shape>
    </item>

</selector>
